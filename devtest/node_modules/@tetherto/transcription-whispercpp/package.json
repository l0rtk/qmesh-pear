{
  "name": "@tetherto/transcription-whispercpp",
  "version": "3.4.3",
  "description": "transcription addon for qvac",
  "addon": true,
  "scripts": {
    "lint": "standard \"examples/**/*.js\" \"test/**/*.js\" \"*.js\"",
    "lint:fix": "standard --fix \"examples/**/*.js\" \"test/**/*.js\" \"*.js\"",
    "build": "bare-make generate && bare-make build && bare-make install",
    "test:unit": "brittle-bare test/unit/**/*.test.js",
    "test:integration": "brittle-bare test/integration/**/*.test.js",
    "test": "npm run test:unit && npm run test:integration",
    "test:dts": "tsc index.d.ts --noEmit"
  },
  "files": [
    "binding.js",
    "index.js",
    "whisper.js",
    "prebuilds",
    "index.d.ts",
    "lib",
    "transcription-addon"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tetherto/qvac-lib-infer-whispercpp.git"
  },
  "author": "Tether",
  "keywords": [
    "tether",
    "addon",
    "whisper",
    "qvac"
  ],
  "license": "Apache-2.0",
  "bugs": "https://github.com/tetherto/qvac-lib-infer-whispercpp/issues",
  "homepage": "https://github.com/tetherto/qvac-lib-infer-whispercpp#readme",
  "devDependencies": {
    "@tetherto/qvac-lib-dl-base": "1.0.2",
    "@tetherto/qvac-lib-dl-filesystem": "1.0.2",
    "@tetherto/qvac-lib-dl-hyperdrive": "^2.2.1",
    "@tetherto/qvac-lib-response": "^1.0.1",
    "bare-buffer": "^2.5.4",
    "bare-fs": "^3.0.2",
    "util": "npm:bare-utils@^1.5.1",
    "bare-stream": "^2.3.0",
    "brittle": "^3.17.0",
    "cmake-bare": "^1.6.5",
    "cmake-vcpkg": "^1.1.0",
    "os": "npm:bare-os@^3.6.2",
    "sinon": "^21.0.0",
    "standard": "^17.0.0",
    "tty": "npm:bare-tty@^5.0.3"
  },
  "dependencies": {
    "@tetherto/infer-base": "^2.4.0",
    "@tetherto/qvac-lib-error-base": "^1.0.0",
    "@tetherto/qvac-util-transcription": "^1.2.0",
    "bare-path": "^3.0.0",
    "process": "npm:bare-process@^4.2.1"
  },
  "peerDependencies": {
    "@tetherto/qvac-lib-dl-hyperdrive": "^2.2.1"
  },
  "exports": {
    "./package": "./package.json",
    "./transcription-addon": "./transcription-addon/index.js",
    ".": "./index.js"
  },
  "types": "index.d.ts"
}
