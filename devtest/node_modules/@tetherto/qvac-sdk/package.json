{
  "name": "@tetherto/qvac-sdk",
  "version": "0.1.0",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./package": "./package.json",
    "./dist/server/worker.js": {
      "import": "./dist/server/worker.js",
      "require": "./dist/server/worker.js"
    },
    "./expo-plugin": "./dist/expo/plugins/index.js",
    "./worker.mobile.bundle": "./dist/worker.mobile.bundle.js"
  },
  "expo": {
    "plugins": [
      "./dist/expo/plugins/index.js"
    ]
  },
  "files": [
    "dist/**/*",
    "!dist/**/*.map",
    "!**/package-lock.json",
    "!**/yarn.lock",
    "!**/pnpm-lock.yaml",
    "!**/bun.lockb"
  ],
  "scripts": {
    "lint": "eslint . --max-warnings=0 && bun run typecheck",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "lint:fix": "eslint . --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "postcompile:aliases": "node ./scripts/resolve-aliases.mjs",
    "build": "rm -rf dist && bun run lint && tsc --project tsconfig.json && bun run postcompile:aliases",
    "build:pack": "bun run build && bun pm pack --destination dist/",
    "watch": "nodemon --watch server --ext ts --exec 'bun run build'",
    "bare:ensureprebuilds:llamacpp": "cp -r node_modules/@tetherto/llm-llamacpp/prebuilds/android-arm64 node_modules/@tetherto/llm-llamacpp/prebuilds/android-ia32 && cp -r node_modules/@tetherto/llm-llamacpp/prebuilds/android-arm64 node_modules/@tetherto/llm-llamacpp/prebuilds/android-arm && cp -r node_modules/@tetherto/llm-llamacpp/prebuilds/android-arm64 node_modules/@tetherto/llm-llamacpp/prebuilds/android-x64",
    "bare:ensureprebuilds:whispercpp": "cp -r node_modules/@tetherto/transcription-whispercpp/prebuilds/android-arm64 node_modules/@tetherto/transcription-whispercpp/prebuilds/android-ia32 && cp -r node_modules/@tetherto/transcription-whispercpp/prebuilds/android-arm64 node_modules/@tetherto/transcription-whispercpp/prebuilds/android-arm && cp -r node_modules/@tetherto/transcription-whispercpp/prebuilds/android-arm64 node_modules/@tetherto/transcription-whispercpp/prebuilds/android-x64",
    "bare:ensureprebuilds:embeddings": "cp -r node_modules/@tetherto/embed-llamacpp/prebuilds/android-arm64 node_modules/@tetherto/embed-llamacpp/prebuilds/android-ia32 && cp -r node_modules/@tetherto/embed-llamacpp/prebuilds/android-arm64 node_modules/@tetherto/embed-llamacpp/prebuilds/android-arm && cp -r node_modules/@tetherto/embed-llamacpp/prebuilds/android-arm64 node_modules/@tetherto/embed-llamacpp/prebuilds/android-x64",
    "bare:ensureprebuilds": "bun run bare:ensureprebuilds:llamacpp && bun run bare:ensureprebuilds:whispercpp && bun run bare:ensureprebuilds:embeddings",
    "prepare": "bun run husky",
    "bare:example": "./scripts/run-with-bare.sh",
    "docs_api:website": "typedoc --out ./docs/api/website --entryPoints index.ts --entryPointStrategy expand --readme none",
    "docs_api:markdown": "typedoc --plugin typedoc-plugin-markdown --out ./docs/api/markdown --entryPoints index.ts --entryPointStrategy expand --readme none && sed -i '1,4d' ./docs/api/markdown/README.md",
    "docs_api": "bun run docs_api:website && bun run docs_api:markdown"
  },
  "dependencies": {
    "@tetherto/embed-llamacpp": "2.2.4",
    "@tetherto/llm-llamacpp": "2.5.0",
    "@tetherto/qvac-lib-decoder-audio": "1.5.0",
    "@tetherto/qvac-lib-error-base": "1.1.1",
    "@tetherto/qvac-lib-langdetect-text": "1.0.0",
    "@tetherto/qvac-lib-logging": "1.0.3",
    "@tetherto/transcription-whispercpp": "3.4.3",
    "@tetherto/translation-nmtcpp": "2.0.0",
    "fast-safe-stringify": "2.1.1",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@expo/config-plugins": "^8.0.0",
    "@lancedb/lancedb": "^0.21.3",
    "@sqliteai/sqlite-wasm": "3.50.4-sync.0.8.30-vector.0.9.23",
    "@tetherto/qvac-lib-rag": "3.0.0",
    "@types/node": "^24.2.1",
    "bare-pack": "^1.4.2",
    "bare-readline": "^1.1.0",
    "chromadb": "^3.0.14",
    "expo": "^51.0.0",
    "eslint": "^9.33.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "hyperdb": "^4.16.1",
    "prettier": "^3.6.2",
    "typedoc": "^0.28.11",
    "typedoc-plugin-markdown": "^4.8.1",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.0"
  },
  "peerDependencies": {
    "bare-buffer": "^3.3.0",
    "bare-crypto": "^1.9.2",
    "bare-dns": "^2.1.2",
    "bare-fetch": "^2.5.0",
    "bare-fs": "^4.1.6",
    "bare-http1": "^4.0.4",
    "bare-net": "^2.0.1",
    "bare-os": "^3.6.1",
    "bare-path": "^3.0.0",
    "bare-process": "^4.2.1",
    "bare-rpc": "^0.2.11",
    "bare-runtime": "^1.22.1-0",
    "bare-stream": "^2.6.5",
    "bare-subprocess": "^5.1.1",
    "corestore": "^7.4.5",
    "events": "^3.3.0",
    "hyperdrive": "^13.0.1",
    "hyperswarm": "^4.12.1"
  },
  "optionalDependencies": {
    "expo-file-system": "^18.1.11",
    "react-native-bare-kit": "^0.9.2"
  },
  "patchedDependencies": {
    "bare-rpc@0.2.11": "patches/bare-rpc@0.2.11.patch"
  }
}