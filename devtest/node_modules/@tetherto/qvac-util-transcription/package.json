{
  "name": "@tetherto/qvac-util-transcription",
  "version": "1.2.0",
  "author": "Tether",
  "main": "index.js",
  "scripts": {
    "test:unit:generate": "brittle -r test/unit/all.js test/unit/*.test.js",
    "test:unit": "npm run test:unit:generate && bare test/unit/all.js",
    "test": "npm run test:unit",
    "coverage:unit": "brittle-bare --coverage --cov-dir=coverage/unit test/unit/*.test.js && npx istanbul report html --include=coverage/unit/coverage-final.json",
    "coverage": "brittle-bare --coverage --cov-dir=coverage/unit test/unit/*.test.js && npx istanbul report html --include=coverage/unit/coverage-final.json",
    "lint": "standard",
    "lint:fix": "standard --fix"
  },
  "keywords": [
    "whisper",
    "qvac",
    "transcription",
    "vad",
    "decoder"
  ],
  "license": "ISC",
  "description": "Transcription utility for Qvac, to pipe audio from decoder, to silero vad, to whisper",
  "files": [
    "index.js",
    "index.d.ts",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tetherto/qvac-util-transcription.git"
  },
  "dependencies": {
    "@tetherto/qvac-lib-decoder-audio": "1.4.0",
    "@tetherto/qvac-lib-error-base": "1.1.0",
    "@tetherto/qvac-lib-inference-addon-mlc-base": "^2.2.0",
    "@tetherto/qvac-lib-response": "1.0.1"
  },
  "devDependencies": {
    "@tetherto/qvac-lib-dl-hyperdrive": "^1.0.3",
    "@tetherto/qvac-lib-inference-addon-mlc-whisper-tiny-q0f32": "1.3.3",
    "@tetherto/qvac-lib-inference-addon-onnx-silerovad": "0.4.3",
    "bare-fs": "^4.1.4",
    "bare-stream": "^2.6.5",
    "brittle": "^3.13.1",
    "corestore": "^7.4.1",
    "hyperbee": "^2.24.2",
    "hyperswarm": "^4.11.5",
    "istanbul": "^0.4.5",
    "standard": "^17.1.2"
  },
  "bugs": "https://github.com/tetherto/qvac-util-transcription/issues",
  "exports": {
    "./package": "./package.json",
    ".": "./index.js"
  },
  "types": "index.d.ts"
}
