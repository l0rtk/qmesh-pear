{
  "name": "qmesh-pear",
  "version": "0.1.0",
  "description": "QMesh - Distributed P2P LLM Inference Network (Pear Runtime)",
  "type": "module",
  "main": "index.js",
  "pear": {
    "name": "qmesh-worker",
    "type": "terminal",
    "stage": [],
    "links": {}
  },
  "imports": {
    "#fs": {
      "bare": "bare-fs",
      "default": "node:fs"
    },
    "#fs/promises": {
      "bare": "bare-fs/promises",
      "default": "node:fs/promises"
    },
    "#process": {
      "bare": "bare-process",
      "default": "node:process"
    },
    "#os": {
      "bare": "bare-os",
      "default": "node:os"
    },
    "#path": {
      "bare": "bare-path",
      "default": "node:path"
    },
    "#child_process": {
      "default": "node:child_process"
    }
  },
  "scripts": {
    "dev": "pear run --dev .",
    "stage": "pear stage --channel main",
    "seed": "pear seed",
    "test": "pear run --dev . --test"
  },
  "keywords": [
    "llm",
    "inference",
    "p2p",
    "distributed",
    "ai",
    "pear",
    "bare"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@inquirer/prompts": "^7.0.1",
    "b4a": "^1.6.0",
    "bare-fs": "^2.0.0",
    "bare-node-runtime": "^1.1.4",
    "bare-os": "^2.0.0",
    "bare-path": "^2.0.0",
    "bare-process": "^2.0.0",
    "hyperswarm": "^4.0.0",
    "node-llama-cpp": "^3.1.1"
  }
}
